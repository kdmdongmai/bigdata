package com.lec.ex;

public class VarEx02 {
	public static void main(String[] args) {
		int i = 10; //4byte짜리 i주머니에 10을 넣는다.
		byte j = 20; // 1byte짜리 j주머니에 20을 넣는다.
		double d = 1; // 8byte짜리 실수 d주머니에 1.0을 할당한다.
		char c = 'c'; // 2byte짜리 문자 c주머니에 'c'를 할당
		boolean ok = true;
		long l = 22000000;
		
		System.out.printf("%.d", l);
	}
}
